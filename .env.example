# Weights & Biases API Key (Found at https://wandb.ai/authorize)
WANDB_API_KEY=wandb_v1_your_key_here

# Azure Storage Connection String (Required for 'dvc pull')
# Format: DefaultEndpointsProtocol=https;AccountName=...;AccountKey=...;EndpointSuffix=core.windows.net
AZURE_STORAGE_CONNECTION_STRING="your_connection_string_here"

# GitHub Credentials (Optional: For cloning private repos or pushing without typing passwords)
# Token requires 'repo' scope. Generate at https://github.com/settings/tokens
GITHUB_USERNAME=your_username
GITHUB_TOKEN=ghp_your_token_here
